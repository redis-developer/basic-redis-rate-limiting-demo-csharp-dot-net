{
  "name": "Basic Redis Rate Limiting Demo .Net Core 5",
  "stack": "container",
  "addons":[
    "rediscloud"
  ],
  "env": {
    "REDIS_ENDPOINT_URL": {
      "description": "Redis server host:port, ex: 127.0.0.1:6379",
      "required": true
    },
    "REDIS_PASSWORD": {
      "description": "Redis server password",
      "required": true
    }
  }
}
